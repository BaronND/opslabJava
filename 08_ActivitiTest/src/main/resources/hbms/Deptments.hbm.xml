<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.opslab.workflow.framework.entity.business.Deptments" table="tbl_hbm_deptments">
        <id name="id" type="java.lang.String" length="32">
            <column name="ID"/>
            <generator class="assigned"/>
        </id>
        <property name="deptId" type="java.lang.String" length="32">
            <column name="deptId"/>
        </property>
        <property name="detpName" type="java.lang.String" length="100">
            <column name="detpName"/>
        </property>
        <property name="valid" type="java.lang.String" length="1">
            <column name="valid"/>
        </property>
        <property name="parentId" type="java.lang.String" length="32">
            <column name="parentId"/>
        </property>
    </class>
</hibernate-mapping>
